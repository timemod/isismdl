setgo(output=hdoff);

per = 1000y;
waarnemingen = [0.412 0.202 0.813 0.642];
aantal_waarnemingen = length(waarnemingen);
aantal_rms_en = 8;

cmpmdl("non_square");
readmdl("non_square");

(* maak 1e en 4e kolom afhankelijk *)
put_mdl_param;
r1 = r4 * 2;
get_mdl_param;

setmdp(1000y);

(* maak waarnemingen aan *)
for w = waarnemingen; i = 1 .. aantal_waarnemingen do
    (~("w" // i // "_fit"))[per] = w;
endfor;

(* maak de rms-en gelijk aan 1 *)
for i = 1 .. aantal_rms_en do
    ~"x" // i // "_rms" = 1;
endfor;

getfit;
getrms;

setfopt(options = prijac, nowarnca, svdtest);
solve;

setfopt(options = prijac, nowarnca, nosvdtest);
solve;

setfopt(options = prijac, nowarnca);
solve;

setfopt(options = prijac, nowarnca, nochkjac);
solve;

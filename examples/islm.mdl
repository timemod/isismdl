param c0 100 c1 0.7  c2 20 c3 0.5;
param i0 100 i1 0.2  i2 40 i3 1.5;
param m0  75 m1 0.23 m2 35 m3 1.5;
param t0 -25 t1 0.22;

frml t = t0 + t1 * y;
frml i = i0 + i1 * (0.2 * y + 0.8 * y[-1]) - i2 * r[-1] + i3 * r[-1]*r[-1];
frml md = m0 + m1 * y - m2 * r + m3 * r*r;
ident yd = y - t;
frml c = c0 + c1 * (0.9 * yd + 0.1 * yd[-1]) - c2 * r + c3 * r*r;
ident y = c + i + g;


ident r = r + (ms - md) / ms;

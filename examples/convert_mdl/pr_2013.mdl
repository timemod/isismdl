
  ? --------------------------
  ?       proloogmodel
  ? --------------------------

function round(x,p)= nint(x/p)*p ;
function ceil (x,p) = nint(x/p +.5 - 10**(-10))*p  ;
function floor (x,p) = nint(x/p-.5 + 10**(-10))*p ;

ident wimn wimn = wi_nr_mn ;

  ? hieronder een aantal vergelijkingen die voorheen in het belastingmodel van ovf stonden. betreft enkele ih-zaken: winst, opbrengst ih-box2,
  ? verrekende voorheffing dividendbelasting, woz waarde, hypotheekrenteaftrek
ident  ovi_wr = (z_totwn - zzhu_wn) / (z_totwn(-1) - zzhu_wn(-1)) - 1 ;
ident  wi_wr  = zzhu_wn / zzhu_wn(-1) - 1 ;
  ? huizenprijzen 6 jaar vertraagd nodig terwijl isis er slechts 4 toestaat, hier doorschuiven. zoiets gebeurt ook bij rente pensioenfondsen in macromodel.
ident  wh1_pr = wh_pr(-1) ;
ident  wh2_pr = wh1_pr(-1) ;
ident  wh3_pr = wh2_pr(-1) ;
ident  wh4_pr = wh3_pr(-1) ;
ident  wh5_pr = wh4_pr(-1) ;
ident  wh6_pr = wh5_pr(-1) ;
ident  hui_7_pr = (wh_pr + wh1_pr + wh2_pr + wh3_pr + wh4_pr + wh5_pr + wh6_pr) / 7 ;
frml   hwz_wn = hwz_wn(-1) * (1 + hui_7_pr) * hui_aut_pr ;
ident  lv_wdh_wd = lv_hwz_qn * (hwz_wn - hwz_wn(-1) - lv_iwo_qn * iwo_wn) ;
ident  lv_wdk_wd = lv_wdk_1_qn * (lv_wdk_2_qn * wa_pr - lv_wdk_3_qn * wa_pr(-1)) * lv_wz_wn(-1) ;
frml   lv_wz_wn = lv_wz_wn(-1) + lv_wdh_wd + lv_wdk_wd ;

frml   lv_wg_er = (lv_wz_wn + lv_wz_wn(-1)) / (lv_wz_wn(-1) + lv_wz_wn(-2)) - 1 ;
frml   lv_wrs_er = lv_wdh_wd / lv_wz_wn(-1) ;
frml   gibbox2wn = gibbox2wn(-1) * (1 + ovi_wr) + gibbox2_aut_wn;
  ? box3 verhuisd naar macromodel
frml   gvdivwn = gvdivwn(-1) * (1 + ovi_wr) + gvdiv_aut_wn ;
frml   fkewown = fkewown(-1) * (hypren_qn / hypren_qn(-1)) * (hwz_wn + hwz_wn(-1)) / (hwz_wn(-1) + hwz_wn(-2)) + fkewo_aut_wn ;
frml   wi_bimbam_wn = wi_bimbam_wn(-1) * (1 + wi_wr) + wi_bimbam_aut_wn ;
frml   woz_sprong_qn = wh2_pr ;

  ? ib-plichtige winst incl maatregelen
ident  wiwn = wi_bimbam_wn + wi_maatr_wn ;
  ? opbrengst box2 en box4(=verrekende voorheffing dividenbelasting)
ident  ibbox2wn = gibbox2wn * bel_b2_tn ;
ident  vdivwn =  - gvdivwn * db_tn ;

  ? ziekteverzuim wulbz en dso

  ? werkloosheidspercentage prikken voorzover overgenomen uit confrontatie
frml   wbvqn = wbvqn(-1) + wbvautqn ;
  ? conjunctuureffect is factor maal mutatie werkloosheidspercentage. was lange tijd -0,25, tegenwoordig kleiner
ident  zz_conjef_qn = (wbvqn - wbvqn(-1)) * zz_fact_conj_qn ;
  ? mutatie ziekteverzuim is conjunctuur effect jaar t minus afbouw van deel van die van de voorgaande drie jaren
ident  zz_mut_qn = zz_conjef_qn - ( zz_afb1_qn * zz_conjef_qn(-1) + zz_afb2_qn * zz_conjef_qn(-2) + zz_afb3_qn * zz_conjef_qn(-3) ) ;
  ? ziekteverzuimpercentages loontrekkers totaal en overheid
frml   zz_l_qn = zz_l_qn(-1) + zz_mut_qn + zz_l_aut_qn ;
frml   zz_o_qn = zz_o_qn(-1)             + zz_o_aut_qn ;
  ? aandeel dso in bedragen dso+wulbz. in nr-jaren berekenen uit bedragen jaar t: bedrag dso gedeeld door bedrag dso+wulbz
frml   zz_dso_ziek_m_qn = zz_dso_ziek_m_qn(-1) ;
frml   zz_dso_ziek_z_qn = zz_dso_ziek_z_qn(-1) ;
frml   zz_dso_ziek_o_qn = zz_dso_ziek_o_qn(-1) ;
  ? volume ziekteverzuim loontrekkers totaal, overheid en bedrijven
ident  zz_l_mn = zz_l_qn * (bl_m_aj_nn + bl_z_aj_nn + bl_o_aj_nn) ;
ident  lodsomn = zz_o_qn * (bl_m_aj_nn * zz_dso_ziek_m_qn + bl_z_aj_nn * zz_dso_ziek_z_qn + bl_o_aj_nn * zz_dso_ziek_o_qn) ;
ident  zzzbvmn = zz_l_mn - lodsomn ;
  ? bedragen dso en wulbz in loonsomopbouw
frml   dso_m_wn = dso_m_wn(-1) * lodsomn / lodsomn(-1) * (1 + bl_ctr_m_er + bl_inc_str_m_er) ;
frml   dso_z_wn = dso_z_wn(-1) * lodsomn / lodsomn(-1) * (1 + bl_ctr_z_er + bl_inc_str_z_er) ;
frml   dso_o_wn = dso_o_wn(-1) * lodsomn / lodsomn(-1) * (1 + bl_ctr_o_er + bl_inc_str_o_er) ;
ident  dso_b_wn = dso_m_wn + dso_z_wn ;
frml   wulbz_m_wn = wulbz_m_wn(-1) * zzzbvmn / zzzbvmn(-1) * (1 + bl_ctr_m_er + bl_inc_str_m_er) ;
frml   wulbz_z_wn = wulbz_z_wn(-1) * zzzbvmn / zzzbvmn(-1) * (1 + bl_ctr_z_er + bl_inc_str_z_er) ;
frml   wulbz_o_wn = wulbz_o_wn(-1) * zzzbvmn / zzzbvmn(-1) * (1 + bl_ctr_o_er + bl_inc_str_o_er) ;
ident  wulbz_b_wn = wulbz_m_wn + wulbz_z_wn ;

? wegingsfactoren huishoudens in replacement rate en (micro) wiggen
ident mp_mk_qn       = (mp1k_hh_nn + mp2k_hh_nn) / (mp0k_hh_nn + mp1k_hh_nn + mp2k_hh_nn) ;
ident av_hh_nn       = mp0k_hh_nn + mp1k_hh_nn + mp2k_hh_nn + mpov_hh_nn - tv_hh_nn ;
ident rr_weg_aszk_qn = as0k_hh_nn / (as0k_hh_nn + mp_mk_qn * tv_hh_nn + av_hh_nn) ;
ident rr_weg_tvmk_qn = (mp_mk_qn * tv_hh_nn) / (as0k_hh_nn + mp_mk_qn * tv_hh_nn + av_hh_nn) ;
ident rr_weg_avmk_qn = (mp_mk_qn * av_hh_nn) / (as0k_hh_nn + mp_mk_qn * tv_hh_nn + av_hh_nn) ;
ident rr_weg_avzk_qn = 1 - rr_weg_aszk_qn - rr_weg_tvmk_qn - rr_weg_avmk_qn ;

  ? eigen bijdrage zvw: in koopkrachtjaren voor sommige jaren een correctie ivm pakketaanpassingen
ident  zfw_eb_bj_fn  = toreal(jaartal < afschaf_cer_bn)* zfw_eb_bj_ecz_fn + toreal(jaartal >= afschaf_cer_bn)* zfw_eb_bj_icz_fn;
ident  extra_eb2_ch_kk_fn = toreal (jaartal = afschaf_cer_bn) * (zfw_eb_bj_ecz_fn - zfw_eb_bj_icz_fn); 
ident  zfw_eb_fn  = toreal(koopkracht_bn = 0) * zfw_eb_bj_fn  + toreal(koopkracht_bn = 1) * (zfw_eb_bj_fn + zfw_eb_corrkk_fn + extra_eb2_ch_kk_fn);
ident  extra_eb_ch_fn  =  toreal (koopkracht_bn=1) *  extra_eb_ch_kk_fn;


frml   t3_hypren_spec_fn  = t3_hypren_spec_fn(-1) * (1 + woz_w1_er) ;
frml   bezit_spec_fn  = bezit_spec_fn(-1) * (1 + bezit_er) ;
frml   woz_w1_spec_fn  = woz_w1_spec_fn(-1) * (1 + woz_w1_er) ;
frml   huur_spec_fn  = huur_spec_fn(-1) * (1 + huur_er) ;

ident  t3_hypren_spec_v_fn  = t3_hypren_spec_fn(-1) ;
ident  bezit_spec_v_fn  = bezit_spec_fn(-1) ;
ident  woz_w1_spec_v_fn  = woz_w1_spec_fn(-1) ;
ident  huur_spec_v_fn  = huur_spec_fn(-1) ;

  ? totaal wao - tarief
ident  wao_wg_tn  = w1gtn + w2gtn + wkgtn + wko_wg_tn * wko_tlv_aof_bn ;

  ? nominale zvw-premiem tbv micromodel en koopkracht is exogeen
ident  zfw_n_fn  = zfw_nza_fn;

  ? zfw-premie wg afronden, eerst de gewone, dan die met korting
ident  zfw_wg_tn  = round(zfw_wg_onafg_tn, zfw_tar_afrond_tn) ;
ident  zfw_wn_tn2  = round(zfw_wn_onafg_tn2, zfw_tar_afrond_tn) ;
ident  aow_zfw_wg_tn  = zfw_wn_tn2 ;

  ? premiegrens pensioenpremies
frml   pen_pgr_fn  = ceil(pen_pgr_fn(-1) * (brl_1m_wml_fn/ brl_1m_wml_fn(-1)), afr_euro_qn) ;
  ? premiegrens werknemersverzekeringen
frml   wnv_pgr_onaf_fn  = floor((brl_1m_wml_fn / brl_1m_wml_fn(-1)) * wnv_pgr_onaf_fn(-1), afr_cent_qn) ;
frml   wnv_pgr_fn  = floor(wnv_pgr_onaf_fn, afr_euro_qn) ;
frml   wnv_pgr_dag_fn  = floor((wnv_pgr_fn / loon_dagpj_bn), afr_cent_qn) ;

  ? premiegrens zfw/zvw
frml   zfw_pgr_fn  = toreal(zfw_pgr_is_wnv_pgr_bn = 0) * (floor(zfw_pgr_fn(-1) * (1 + (bl_ctr_m_er(-1) + bl_ctr_m_er(-2)) / 2), afr_euro_qn))  +
					toreal(zfw_pgr_is_wnv_pgr_bn = 1) * (wnv_pgr_fn) ;
frml   zfw_pgr_w_fn  = zfw_pgr_fn ;
frml   zfw_pgr_65_fn  = zfw_pgr_fn ;

  ? nb drempelgrens laag/hoog pecentage bu-aftrek, indexeren conform sheet fin: wet ib 2001 obv tabelcorrectiefactor
frml   a_ziek_vzi_hooggr_fn  = ceil(a_ziek_vzi_hooggr_fn(-1) * (1 + tcf_tn * tcf_ih_bn), afr_euro_qn) ;

  ? vermenigvuldigingsfactor van de buitengewone uitgaven
frml   a_ziek_fact_qn  = a_ziek_fact_qn(-1);
frml   a_ziek_fact_65_qn  = a_ziek_fact_65_qn(-1);

ident  a_ziek_eruit_bn  = toreal (koopkracht_bn=0) * a_ziek_eruit_bj_bn + toreal(koopkracht_bn=1) * a_ziek_eruit_kk_bn;


  ? compensatie ivm afschaffen bu voor chronisch zieken en gehandicapten
frml   chff_maxvzi_gr_as_fn  = round((1 + tcf_tn * tcf_ih_bn) * chff_maxvzi_gr_as_fn(-1),afr_cent_qn);
frml   chff_maxvzi_gr_pr_fn  = round((1 + tcf_tn * tcf_ih_bn) * chff_maxvzi_gr_pr_fn(-1),afr_cent_qn);

frml   chff64_gem_fn  =  toreal(koopkracht_bn = 0) * chff_factor_bj_bn *round((1 + tcf_tn * tcf_ih_bn * chff64_gem_index_bn) * chff64_gem_fn(-1),afr_cent_qn) +
						toreal(koopkracht_bn = 1) * chff_factor_pj_bn *round((1 + tcf_tn * tcf_ih_bn * chff64_gem_index_bn) * chff64_gem_fn(-1),afr_cent_qn);
frml   chff64_hoog_fn  = toreal(koopkracht_bn = 0) * chff_factor_bj_bn * round((1 + tcf_tn * tcf_ih_bn * chff64_hoog_index_bn) * chff64_hoog_fn(-1),afr_cent_qn) +
						toreal(koopkracht_bn = 1) * chff_factor_pj_bn * round((1 + tcf_tn * tcf_ih_bn * chff64_hoog_index_bn) * chff64_hoog_fn(-1),afr_cent_qn);
frml   chff65_gem_fn  =  toreal(koopkracht_bn = 0) * chff_factor_bj_bn * round((1 + tcf_tn * tcf_ih_bn * chff65_gem_index_bn) * chff65_gem_fn(-1),afr_cent_qn) +
						toreal(koopkracht_bn = 1) * chff_factor_pj_bn * round((1 + tcf_tn * tcf_ih_bn * chff65_gem_index_bn) * chff65_gem_fn(-1),afr_cent_qn);
frml   chff65_hoog_fn  = toreal(koopkracht_bn = 0) * chff_factor_bj_bn *round((1 + tcf_tn * tcf_ih_bn * chff65_hoog_index_bn) * chff65_hoog_fn(-1),afr_cent_qn) +
						toreal(koopkracht_bn = 1) * chff_factor_pj_bn *round((1 + tcf_tn * tcf_ih_bn * chff65_hoog_index_bn) * chff65_hoog_fn(-1),afr_cent_qn);

  ? percentages voor zorgtoeslag
frml   nrm_pr_zp_qn  = nrm_pr_zp_qn(-1) + nrm_pr_zp_mut_qn;
frml   nrm_pr_mp_qn  = nrm_pr_mp_qn(-1) + nrm_pr_mp_mut_qn;
frml   nrm_pr_qn  = nrm_pr_qn(-1) + nrm_pr_mut_qn;

  ? franchises pensioen en bip
frml   pen_fr_m_fn = round (pen_fr_m_fn(-1) * (1 + bl_ctr_m_er), afr_euro_qn) ;
frml   pen_fr_z_fn = round (pen_fr_z_fn(-1) * (1 + bl_ctr_z_er), afr_euro_qn) ;
frml   pen_fr_o_fn = round (pen_fr_o_fn(-1) * (1 + bl_ctr_o_er), afr_euro_qn) ;
frml   bip_fr_m_fn = round (bip_fr_m_fn(-1) * (1 + bl_ctr_m_er), afr_euro_qn) ;
frml   bip_fr_z_fn = round (bip_fr_z_fn(-1) * (1 + bl_ctr_z_er), afr_euro_qn) ;
frml   bip_fr_o_fn = round (bip_fr_o_fn(-1) * (1 + bl_ctr_o_er), afr_euro_qn) ;


  ? deel loonvoeten / loonsommen

  ? structuureffect frml   voor scheef prikken
frml   bl_inc_str_m_er  = 0 ;
frml   bl_inc_str_z_er  = 0 ;
frml   bl_inc_str_o_er  = 0 ;

  ? volume mutatie, in proloog
ident  bl_m_aj_nr  = bl_m_aj_nn / bl_m_aj_nn(-1) - 1 ;
ident  bl_z_aj_nr  = bl_z_aj_nn / bl_z_aj_nn(-1) - 1 ;
ident  bl_o_aj_nr  = bl_o_aj_nn / bl_o_aj_nn(-1) - 1 ;

ident  bl_m_ur_nr  = bl_m_ur_nn / bl_m_ur_nn(-1) - 1 ;
ident  bl_z_ur_nr  = bl_z_ur_nn / bl_z_ur_nn(-1) - 1 ;
ident  bl_o_ur_nr  = bl_o_ur_nn / bl_o_ur_nn(-1) - 1 ;

  ? volume mutatie bedrijven gewogen met loonkosten vorig jaar
ident  bl_b_aj_nr  = ( (bl_m_aj_nn / bl_m_aj_nn(-1)) * lns_nr_m_wn(-1) + (bl_z_aj_nn / bl_z_aj_nn(-1)) * lns_nr_z_wn(-1) ) / lns_nr_b_wn(-1) - 1 ;

ident  bl_b_ur_nr  = ( (bl_m_ur_nn / bl_m_ur_nn(-1)) * lns_nr_m_wn(-1) + (bl_z_ur_nn / bl_z_ur_nn(-1)) * lns_nr_z_wn(-1) ) / lns_nr_b_wn(-1) - 1 ;

ident  inh_m_pr  = inh_m_wn / max(0.00001,inh_m_wn(-1)) / (1 + bl_m_ur_nr) - 1 ;
ident  inh_o_pr  = inh_o_wn / max(0.00001,inh_o_wn(-1)) / (1 + bl_o_ur_nr) - 1 ;
ident  inh_z_pr  = inh_z_wn / max(0.00001,inh_z_wn(-1)) / (1 + bl_z_ur_nr) - 1 ;

  ? schaalloon
ident  bl_m_wn  = bl_m_wn(-1) * (1 + sl_m_er) * (1 + bl_m_ur_nr) ;
ident  bl_z_wn  = bl_z_wn(-1) * (1 + sl_z_er) * (1 + bl_z_ur_nr) ;
ident  bl_o_wn  = bl_o_wn(-1) * (1 + sl_o_er) * (1 + bl_o_ur_nr) ;
ident  bl_b_wn  = bl_m_wn + bl_z_wn ;
ident  bl_b_er  = (bl_b_wn / bl_b_wn(-1)) / (1 + bl_b_ur_nr) - 1 ;

  ? inhouding (laten bestaan: werknemerspremie awf is nul vanaf 2009, maar kan zomaar terugkeren)
frml   inh_m_wn  = inh_m_wn(-1) / awf_wn_tn(-1) * (1 + bl_m_ur_nr) * (1 + sl_m_er)  * awf_wn_tn ;
frml   inh_z_wn  = inh_z_wn(-1) / awf_wn_tn(-1) * (1 + bl_z_ur_nr) * (1 + sl_z_er)  * awf_wn_tn ;
frml   inh_o_wn  = inh_o_wn(-1) / awf_wn_tn(-1) * (1 + bl_o_ur_nr) * (1 + sl_o_er)  * awf_wn_tn ;
ident  inh_b_wn  = inh_m_wn + inh_z_wn ;
ident  inh_b_pr  = (inh_b_wn / inh_b_wn(-1)) / (1 + bl_b_ur_nr) - 1 ;

  ? brutoloon (cf cpb)
ident  bl_cpb_m_wn  = bl_m_wn - inh_m_wn ;
ident  bl_cpb_z_wn  = bl_z_wn - inh_z_wn ;
ident  bl_cpb_o_wn  = bl_o_wn - inh_o_wn ;
ident  bl_cpb_b_wn  = bl_cpb_m_wn + bl_cpb_z_wn ;
ident  bl_cpb_m_pr  = (bl_cpb_m_wn / bl_cpb_m_wn(-1)) / (1 + bl_m_ur_nr) - 1 ;
ident  bl_cpb_z_pr  = (bl_cpb_z_wn / bl_cpb_z_wn(-1)) / (1 + bl_z_ur_nr) - 1 ;
ident  bl_cpb_o_pr  = (bl_cpb_o_wn / bl_cpb_o_wn(-1)) / (1 + bl_o_ur_nr) - 1 ;
ident  bl_cpb_b_pr  = (bl_cpb_b_wn / bl_cpb_b_wn(-1)) / (1 + bl_b_ur_nr) - 1 ;

  ? arbeidsproductiviteit sector en bedrijfstak overheid
frml   arbprod_o_pr  = toreal(laatste_draaijaar_kk_bn = 0) * ((bl_inc_str_o_er(-1) + bl_inc_str_o_er + bl_inc_str_o_er(1)) / 3) +
						toreal(laatste_draaijaar_kk_bn = 1) * ((bl_inc_str_o_er(-1) + bl_inc_str_o_er) / 2) ;
frml   arbprod_o_sect_pr  = arbprod_o_pr ;
  
  ? gemiddeld loon bedrijven (gebruikt bij specials) merk op dat dit ding _bedr_ heet, maar dat wordt opgehoogd met iets van markt 
frml   gem_loon_bedr_fn  = gem_loon_bedr_fn(-1) * (1 + bl_ctr_m_er + bl_inc_str_m_er) ;

  ? zw-effecten in incidenteel loon op nul na overgang van arbeidsjaren op gewerkte uren.
frml   bl_inc_zw_b_er  = 0 ;
frml   bl_inc_zw_m_er  = bl_inc_zw_b_er ;
frml   bl_inc_zw_z_er  = bl_inc_zw_b_er ;
frml   bl_inc_zw_o_er  = 0 ;


ident  rl_rn_5jrgem_qn  = (rl_rn_qn(-4) + rl_rn_qn(-3) + rl_rn_qn(-2) + rl_rn_qn(-1) + rl_rn_qn) / 5;
ident  mut_rl_rn_5jrgem_qn  = rl_rn_5jrgem_qn/ rl_rn_5jrgem_qn(-1) - 1;
ident  mut_hypren_qn  = (hypren_qn / hypren_qn(-1) -1);
ident  spaarren_berekend_qn  = spaarren_factor_rl_rn_qn * rl_rn_qn + spaarren_const_rl_rn_bn;
ident  spaarren_qn  = spaarren_bereken_bn *  spaarren_berekend_qn  + (1-spaarren_bereken_bn) * spaarren_fisim_qn;
ident  mut_spaarren_qn  = (spaarren_qn / spaarren_qn(-1)-1);

  ? zes-jaars vertraagde nodig voor box3 tarief op sparen
ident  spaarren_1_qn  = spaarren_qn(-1);
ident  spaarren_2_qn  = spaarren_1_qn(-1);
ident  spaarren_3_qn  = spaarren_2_qn(-1);
ident  spaarren_4_qn  = spaarren_3_qn(-1);
ident  spaarren_5_qn  = spaarren_4_qn(-1);
ident  spaarren_6_qn  = spaarren_5_qn(-1);

  ? ib box3 tarieven sparen en beleggen
ident  b3_sp_5jrgem_qn = (spaarren_6_qn + spaarren_5_qn + spaarren_4_qn + spaarren_3_qn + spaarren_2_qn) / 5 ;
ident  b3_sp_reken_qn = b3_sp_fact1_qn * b3_sp_5jrgem_qn - b3_sp_fact2_qn * (b3_sp_5jrgem_qn + b3_sp_fact3_qn) ;
frml   b3_rdmsp_qn = round(b3_rdmsp_qn(-1) * b3_sp_reken_qn / b3_sp_reken_qn(-1) , 0.0001) ;

frml   b3_blg_aan_qn = ( (1 + b3_blg_aan_qn(-1))**14 * (1 + wa_pr(-2)) )**(1/15) - 1 ;
frml   b3_blg_ozk_qn = ( (1 + b3_blg_ozk_qn(-1))**14 * (1 + wh_pr(-2)) )**(1/15) - 1 ;
frml   b3_blg_obl_qn = ( (1 + b3_blg_obl_qn(-1))**14 * (1 + rl_rn_qn(-2)) )**(1/15) - 1 ;
frml   b3_rdmblg_qn = round(b3_blg_gw_aan_qn * b3_blg_aan_qn + b3_blg_gw_ozk_qn * b3_blg_ozk_qn + b3_blg_gw_obl_qn * b3_blg_obl_qn , 0.0001) ;


  ? indexatie primaire inkomensbestanddelen

ident  bl_m_er   = bl_ctr_m_er + bl_inc_str_m_er + bl_m_aut_er + incid_in_kk_bn * toreal(koopkracht_bn = 1) * (bl_inc_conj_er + bl_inc_str_er) ;
ident  bl_z_er   = bl_ctr_z_er + bl_inc_str_z_er + bl_z_aut_er + incid_in_kk_bn * toreal(koopkracht_bn = 1) * (bl_inc_conj_er + bl_inc_str_er) ;
ident  bl_o_er   = bl_ctr_o_er + bl_inc_str_o_er + bl_o_aut_er + incid_in_kk_bn * toreal(koopkracht_bn = 1) * (bl_inc_conj_er + bl_inc_str_er) ;
ident  sl_m_er   = bl_m_er + bl_inc_zw_m_er - bl_m_aut_er ;
ident  sl_z_er   = bl_z_er + bl_inc_zw_z_er ;
